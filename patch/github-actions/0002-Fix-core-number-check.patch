From 321f9fe970bda589fc394e51c2f68c309644db2c Mon Sep 17 00:00:00 2001
From: Wayback Archiver <66856220+waybackarchiver@users.noreply.github.com>
Date: Fri, 19 Nov 2021 15:17:57 +0000
Subject: [PATCH 2/2] Fix core number check

---
 build_darwin_arm64.sh  | 2 +-
 build_darwin_x86_64.sh | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/build_darwin_arm64.sh b/build_darwin_arm64.sh
index 7c90792..49bd05e 100755
--- a/build_darwin_arm64.sh
+++ b/build_darwin_arm64.sh
@@ -9,7 +9,7 @@ XCODE_INCLUDE="$SDK_PATH/usr/include/"
 
 if [ $# -eq 1 ]; then
   re='^[0-9]+$'
-  if  echo "$1" | grep -Eq "$re" ; then
+  if ! echo "$1" | grep -Eq "$re" ; then
     echo "Invalid number of cores" >&2; exit 1
   fi
   jobs=$1
diff --git a/build_darwin_x86_64.sh b/build_darwin_x86_64.sh
index 743e806..ea18a82 100755
--- a/build_darwin_x86_64.sh
+++ b/build_darwin_x86_64.sh
@@ -9,7 +9,7 @@ XCODE_INCLUDE="$SDK_PATH/usr/include/"
 
 if [ $# -eq 1 ]; then
   re='^[0-9]+$'
-  if  echo "$1" | grep -Eq "$re" ; then
+  if ! echo "$1" | grep -Eq "$re" ; then
     echo "Invalid number of cores" >&2; exit 1
   fi
   jobs=$1
